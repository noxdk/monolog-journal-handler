{
    "name": "noxdk/monolog-journal-handler",
    "version": "1.0.0",
    "description": "Monolog handler for journald (systemd) written in pure PHP",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "ext-sockets": "*",
        "monolog/monolog": "^3.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "phpstan/phpstan-strict-rules": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "Noxdk\\MonologJournalHandler\\": "src/"
        }
    },
    "scripts": {
        "analyze": "vendor/bin/phpstan analyse -c phpstan.neon"
    }
}
